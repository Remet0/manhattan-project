{
  "name": "manhattan-project",
  "version": "1.0.0",
  "description": "A MERN stack application for connection people with their poison",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node server.ts",
    "server": "nodemon -x ts-node server.ts",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmcxii/manhattan-project.git"
  },
  "author": "Team Taco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmcxii/manhattan-project/issues"
  },
  "homepage": "https://github.com/mmcxii/manhattan-project#readme",
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/mongoose": "^5.5.19",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.11",
    "express": "^4.17.1",
    "mongoose": "^5.7.3",
    "morgan": "^1.9.1",
    "react-overdrive": "0.0.10",
    "ts-mongoose": "0.0.19",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3"
  }
}
